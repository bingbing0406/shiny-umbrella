#include<stdio.h>
const int 5;
struct salesperson
{
	char name[15];
	float task,sales,bonus;
}persons[n]={"emp1",50,35,0,"emp2",50,48,0,"emp3",50,18,0,"emp4",50,42.5,0,"emp5",50,60,0};
int personbonus()
{
	int i,p;
	double p=0;
	for(i=1;i<=5;i++)
	{
		p=person[i].sales/person[i].task;
	}
	if(p<=0.4)   person[i].bonus=0;
	else if(p>=0.4&&p<0.8)    person[i].bonus=(p-0.4)*0.08*50;
	else    person[i].bonus=(p-0.8)*0.01*50+0.4*0.08*50;
}
